@model SocialModel
<ul class="networks">

    <li class="instagramIcon">
        <a href="https://www.instagram.com/livestore33" target="_blank">
            <img src="../Themes/DefaultClean/Content/images/instagram.png" class="instagramIconImage" alt="" title=""/> 
         </a>
    </li>

    @if (!string.IsNullOrEmpty(Model.FacebookLink))
    {
        <li class="facebook">
            <a href="@Model.FacebookLink" target="_blank">
                <i class="fa fa-facebook-square" aria-hidden="true"></i>
            </a>
        </li>
    }
    @if (!string.IsNullOrEmpty(Model.TwitterLink))
    {
        <li class="twitter">
            <a href="@Model.TwitterLink" target="_blank">
                <i class="fa fa-twitter-square" aria-hidden="true"></i>
            </a>
        </li>
    }
    @*@if (Model.NewsEnabled)
        {
            <li class="rss"><a href="@Url.RouteUrl("NewsRSS", new {languageId = Model.WorkingLanguageId})">@T("Footer.FollowUs.RSS")</a></li>
        }*@
    @if (!string.IsNullOrEmpty(Model.YoutubeLink))
    {
        <li class="youtube">
            <a href="@Model.YoutubeLink" target="_blank">
                <i class="fa fa-youtube-play" aria-hidden="true"></i>
            </a>
        </li>
    }
</ul>